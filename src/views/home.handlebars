
<div>
    <h1>Lista de productos</h1>
    <ul>
        {{#each productos}}
            <li>
                <h2>{{title}}</h2>
                <p>{{description}}</p>
                <p>Código: {{code}}</p>
                <p>Precio: ${{price}}</p>
                <!-- Agrega más información según sea necesario -->
            </li>
        {{else}}
            <p>No hay productos disponibles.</p>
        {{/each}}
    </ul>
</div>

